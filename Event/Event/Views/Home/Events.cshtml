@model IEnumerable<Event.Models.MyEvent>

@{
    ViewBag.Title = "Events";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title.</h2>

<ul>
    @foreach (var p in Model)
    {
        <li>@Html.ActionLink(p.Name, "ViewProduct","Home", new { EventName = p.Name }, null)</li>
    }


</ul>




